{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "medicinemanager-api",
      "image": "ahmedi999/medicinesmanager-api:12.01.2024.17.29.37",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://URLHERE:5332/medicinemanager"
        },
        {
          "name": "SPRING_JPA_hibernate_ddl-auto",
          "value": "update"
        },
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://URLHERE:5432/medicinemanagertest"
        },
        {
          "name": "SPRING_JPA_hibernate_ddl-auto",
          "value": "update"
        }
      ]
    },
    {
      "name": "medicinesmanager-react",
      "image": "ahmedi999/medicinesmanager-react:17.01.2024.17.59.12",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5173
        }
      ]
    }
  ]
}
