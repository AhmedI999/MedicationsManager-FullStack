{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "medicinemanager-api",
      "image": "ahmedi999/medicinesmanager-api:latest",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
     "environment": [{
       "name": "SPRING_DATASOURCE_URL",
       "value": "jdbc:postgresql://URLHERE:5432/medicinemanager"
     },
       {
        "name": "SPRING_JPA_hibernate_ddl-auto",
         "value": "update"
       }
     ]
    }
  ]
}